<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="person1" class="com.dto.Person">
	<constructor-arg name="name" value="홍길동"></constructor-arg>
	<constructor-arg name="age" value="100"></constructor-arg>
	<constructor-arg name="gender" value="male"></constructor-arg>
	<constructor-arg name="CatName" ref="cat1"></constructor-arg>
	</bean>
	
	<bean id="person2" class="com.dto.Person">
	<constructor-arg name="name" value="유관순"></constructor-arg>
	<constructor-arg name="age" value="100"></constructor-arg>
	<constructor-arg name="CatName" ref="cat2"></constructor-arg>
	</bean> 
	
	<bean id="cat1" class="com.dto.Cat">
	<constructor-arg name="CatName" value="나비"></constructor-arg>
	</bean>
	
	<bean id="cat2" class="com.dto.Cat">
	<constructor-arg name="CatName" value="야옹이"></constructor-arg>
	</bean>
	
</beans> 
