<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="firstStudent" class="com.dto.Student">
	<constructor-arg name="name" value="홍길동"></constructor-arg> <!-- 생성자를 호출하는 태그, 여기서의 x는  -->
	</bean>
	
	<bean id="secondStudent" class="com.dto.Student">
	<constructor-arg name="age" value="20"></constructor-arg> <!-- dto에서 age는 Type이 int지만 여기서 자동 변환된다. -->
	</bean>
	
	<bean id="thirdStudent" class="com.dto.Student">
	<constructor-arg name="name" value="유관순"></constructor-arg>
	<constructor-arg name="age" value="20"></constructor-arg>
	</bean> 
	
	
</beans> 
